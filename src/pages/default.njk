---
layout: base.njk
pagination:
    data: wpages
    size: 1
    alias: wpage
eleventyComputed:
  yoastHead: "{{ wpage.yoast|safe }}"
permalink: /{{ wpage.slug }}/index.html
---
{%- for item in pagination.items %}
    {% if item.template == "templates/home-page.php" %}
        {% include "pages/home.njk" %}
    {% elseif item.template == "templates/about.php" %}
        {% include "pages/about.njk" %}
    {% elseif item.template == "templates/our-services.php" %}
        {% include "pages/our-services.njk" %}
    {% elseif item.template == "templates/our-team.php" %}
        {% include "pages/our-team.njk" %}
    {% elseif item.template == "templates/contact.php" %}
        {% include "pages/contact.njk" %}
    {% elseif item.template == "templates/thank-you.php" %}
        {% include "pages/thank-you.njk" %}
    {% endif %}
{% endfor -%}