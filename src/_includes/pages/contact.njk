<style>
    .hero-content-section {
        background-color: #323F32;
        clip-path: polygon(30% 0%, 70% 0%, 100% 0, 100% 75%, 75% 100%, 0 100%, 0% 70%, 0 0);
    }
    .hero-section-box {
        max-width: 89rem;
    }
    .menu-item {
        color: #323F32;
    }
    .menu-last {
        background-color: #323F32;
        color: #EDEEE9;
        letter-spacing: 3px;
        padding: 6px 15px 4px 15px;
    }
    .is-sticky .menu-item {
        color: #EDEEE9;
    }
    .is-sticky .menu-last {
        background-color: #EDEEE9;
        color: #323F32;
    }
    .contact-form-section {
        background-color: #EDEEE9;
    }
    .cog-label {
        font-family: commuters-sans, sans-serif !important;
        font-size: 10px !important;
        font-weight: 600 !important;
        text-transform: uppercase !important;
        letter-spacing: 1.5px !important;
        color: #323F32 !important;
    }
    .cog-input input {
        background-color: transparent;
        border-bottom: 2px solid #323F32;
        font-family: 'Satoshi' !important;
        padding: 12px 0 12px 0;
    }
    .cog-input select {
        background-color: transparent;
        border-bottom: 2px solid #323F32;
        font-family: 'Satoshi' !important;
        padding: 12px 0 12px 0;
    }
    .cog-input textarea {
        background-color: transparent;
        border-bottom: 2px solid #323F32;
        font-family: 'Satoshi' !important;
        padding: 12px 0 12px 0;
    }
    .cog-button--submit {
        background-color: #323F32;
        color: #EDEEE9;
        letter-spacing: 3px;
        padding: 6px 15px 4px 15px;
        margin-left: auto !important;
    }
</style>

